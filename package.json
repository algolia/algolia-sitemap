    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.6.1",
    "eslint": "3.19.0",
    "eslint-config-algolia": "8.0.0",
    "husky": "0.13.4",
    "jest": "20.0.4",
    "jest-serializer-html": "4.0.1",
    "lint-staged": "3.6.1",
    "prettier": "1.7.4"
